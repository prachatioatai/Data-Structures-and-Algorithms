public class Main {

    public static class TreeNode {
        int data;
        TreeNode left;
        TreeNode right;

        public TreeNode(int data) {
            this.data = data;
            this.left = null;
            this.right = null;
        }
    }

    public static TreeNode createNewNode(int data) {
        return new TreeNode(data);
    }
     public static void inOrderTraversal(TreeNode node) {
        if (node == null) {
            return;
        }
        inOrderTraversal(node.left);
        System.out.print(node.data + ", ");
        inOrderTraversal(node.right);
    }
    
    public static void preOrderTraversal(TreeNode node) {
        if (node == null) {
            return;
        }
        System.out.print(node.data + ", ");
        preOrderTraversal(node.left);
        preOrderTraversal(node.right);
    }
    
    public static void postOrderTraversal(TreeNode node) {
        if (node == null) {
            return;
        }
        postOrderTraversal(node.left);
        postOrderTraversal(node.right);
        System.out.print(node.data + ", ");
    }

    public static void main(String[] args) {
        TreeNode root = createNewNode(5);
        
        TreeNode node12 = createNewNode(12);
        TreeNode node13 = createNewNode(13);
        
        TreeNode node7 = createNewNode(7);
        TreeNode node14 = createNewNode(14);
        TreeNode node2 = createNewNode(2);
        
        TreeNode node17 = createNewNode(17);
        TreeNode node23 = createNewNode(23);
		TreeNode node27 = createNewNode(27);
        TreeNode node3 = createNewNode(3);
        TreeNode node8 = createNewNode(8);
        TreeNode node11 = createNewNode(11);
        
        root.left = node12;
		root.right = node13;

        node12.left = node7;

        node13.left = node14;
        node13.right = node2;

        node7.left = node17;
        node7.right = node23;

        node14.left = node27;
        node14.right = node3;

        node2.left = node8;
        node2.right = node11;
    
    System.out.println("INORDER");
    inOrderTraversal(root);
  	System.out.println("");
    System.out.println("PREORDER");
    preOrderTraversal(root);
    System.out.println("");
    System.out.println("POSTORDER");
    postOrderTraversal(root);
    }
   }
